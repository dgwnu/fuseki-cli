{
  "name": "@dgwnu/fuseki-cli-dev",
  "version": "0.0.1",
  "description": "Fuseki NPM CLI application development in TypeScript",
  "cli": "ts-node -P ts-node-config.json bin/fuseki-cli.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:cli",
    "build:cli": "tsc -p ts-cli-config.json",
    "build:lib": "rollup -c",
    "run": "fuseki-service-cli run",
    "start": "fuseki-service-cli start && pm2 l",
    "stop": "fuseki-service-cli stop && pm2 l",
    "restart": "fuseki-service-cli restart && pm2 l",
    "pm2:list": "pm2 l",
    "cli:ping": "$npm_package_cli ping",
    "cli:server": "$npm_package_cli server",
    "cli:datasets": "$npm_package_cli datasets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgwnu/tripple-stores.git"
  },
  "keywords": [
    "fuseki",
    "cli",
    "development",
    "typescript"
  ],
  "author": "Onno Haldar <onno@haldar.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgwnu/tripple-stores/issues"
  },
  "homepage": "https://github.com/dgwnu/tripple-stores#readme",
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@dgwnu/fuseki-service": "git+https://github.com/dgwnu/fuseki-service.git",
    "@types/npm": "^2.0.31",
    "rollup": "^2.35.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^3.0.0",
    "rxjs": "^6.6.3"
  }
}
